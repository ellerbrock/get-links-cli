{
  "name": "get-links-cli",
  "description": "get links via dom selector (cli version)",
  "version": "0.0.5",
  "author": "Maik Ellerbrock <opensource@frapsoft.com> (https://github.com/ellerbrock)",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "url": "git@github.com:ellerbrock/get-links-cli.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/ellerbrock/get-links-cli/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "start": "nodemon src --exec babel-node",
    "test": "eslint src"
  },
  "files": [
    "dist"
  ],
  "preferGlobal": "true",
  "bin": {
    "get-links": "dist/index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "production": {
        "presets": [
          "es2015",
          "babili"
        ]
      }
    }
  },
  "extends": "standard",
  "installedESLint": true,
  "plugins": [
    "standard",
    "promise"
  ],
  "keywords": [
    "collect-links",
    "get-links",
    "get-links-cli",
    "get-urls",
    "get-urls-cli",
    "href",
    "link-css",
    "link-parser",
    "links",
    "url-css",
    "urls"
  ],
  "dependencies": {
    "commander": "^2.9.0",
    "console-emoji": "^0.0.2",
    "get-links": "^0.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babili": "^0.1.0",
    "cross-env": "^5.1.2",
    "eslint": "3.16.1",
    "eslint-config-standard": "7.0.0-beta.0",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.2.0",
    "rimraf": "^2.5.4"
  }
}
